<%= form_for @new_post, html: { multipart: true } do |f| %>
  <%= f.label :title %><br />
  <%= f.text_field :title %> <br />
  
  <%= f.label :tag_list, 'Tags' %> <br />
  <%= f.text_field :tag_list %> <br />
  
  <%= f.label :picture %><br />
  <%= f.file_field :picture %><br />

  <%= f.submit 'Post' %>
<% end %>